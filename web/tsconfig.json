{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@components/*": ["./src/components/*"],
			"@lib/*": ["./src/lib/*"],
			"@assets/*": ["./src/assets/*"],
			"$lib": ["./src/lib/"],
			"$lib/*": ["./src/lib/*"],
		}
	}
}
